<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appenginedemo"
    android:installLocation="auto">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/logo1"
        android:largeHeap="true"
        android:theme="@android:style/Theme.Light.NoTitleBar"
        android:vmSafeMode="true">
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.SplashmainScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.MainActivity"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:label="SplashmainScreen" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.ActivityCamera"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout"
            android:label="ActivityCamera" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.ActivityBase"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:label="ActivityBase" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.ActivityCamera1"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout"
            android:label="ActivityCamera1" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.Activity_3d"
            android:label="Activity_3d" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.MultiPhotoSelectActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout"
            android:label="MultiSelectActivity" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.Activity_Sample"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout"
            android:label="Activity_Sample" />
        <activity
            android:name="excavation.excavation_app.com.appenginedemo.ActivityImageProperty"
            android:configChanges="orientation|keyboard|keyboardHidden|screenLayout"
            android:label="Activity_Image_Property" />
    </application>
</manifest>